{
  "name": "family-calendar-2.0",
  "version": "1.0.0",
  "description": "Family Calendar mobile app built with Expo and React Native",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "eas build",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "submit": "eas submit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FlamingHalapeno/Family-Calendar-2.0.git"
  },
  "keywords": [
    "expo",
    "react-native",
    "typescript",
    "family",
    "calendar",
    "mobile"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FlamingHalapeno/Family-Calendar-2.0/issues"
  },
  "homepage": "https://github.com/FlamingHalapeno/Family-Calendar-2.0#readme",
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-navigation/drawer": "^7.4.1",
